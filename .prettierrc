{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 80,
	"plugins": [
		"prettier-plugin-organize-imports",
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss"
	],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"organizeImportsSkipDestructiveCodeActions": true
}
